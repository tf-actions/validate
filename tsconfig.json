{
	"compilerOptions": {
		"target": "ES2022",
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"declaration": true,
		"strict": true,
		"strictNullChecks": true,
		"incremental": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"rootDir": "./src",
		"outDir": "./dist",
		"composite": true
	},
	"include": ["src"]
}
